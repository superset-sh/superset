{
	"name": "@superset/cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": {
		"superset": "./dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "bun build src/cli.tsx --outfile=dist/cli.js --target=bun && sed -i '' '1s|.*|#!/usr/bin/env bun|' dist/cli.js && chmod +x dist/cli.js",
		"install:global": "bun run build && ln -sf \"$(pwd)/dist/cli.js\" ~/.bun/bin/superset",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"dev": "tsc --watch",
		"start": "bun src/server.ts",
		"typecheck": "tsc --noEmit",
		"test": "bun test"
	},
	"files": [
		"dist",
		"src/types"
	],
	"exports": {
		"./types/*": "./src/types/*",
		"./types": "./src/types/index.ts"
	},
	"dependencies": {
		"@superset/git-utils": "workspace:*",
		"@superset/local-db": "workspace:*",
		"commander": "^14.0.2",
		"drizzle-orm": "0.45.1",
		"uuid": "^13.0.0",
		"ink": "^6.5.0",
		"ink-select-input": "^6.2.0",
		"ink-table": "^3.1.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"react": "^19.2.3",
		"react-devtools-core": "^7.0.1",
		"string-width": "^8.1.0"
	},
	"devDependencies": {
		"@superset/typescript": "workspace:*",
		"@types/react": "^19.2.7",
		"bun-types": "^1.3.1",
		"chalk": "^5.6.2",
		"chokidar": "^3.5.3",
		"ink-testing-library": "^3.0.0",
		"typescript": "^5.9.3"
	}
}
