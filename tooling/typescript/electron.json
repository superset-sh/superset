{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"jsx": "react-jsx",
		"importHelpers": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"noUncheckedIndexedAccess": false
	}
}
